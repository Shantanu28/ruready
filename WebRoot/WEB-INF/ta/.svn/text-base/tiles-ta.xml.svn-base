<tiles-definitions>

	<!-- Base layout of all pages -->
	<!--
		Note 1: the "title" and "header" fields are keys used by <bean:message>, not literal strings.
	-->
	<definition name=".commonDef"
		path="/WEB-INF/common/tiles/template.jsp">
		<put name="title" value="A key into Application.resources" />
		<put name="header" value="app.header" />
		<put name="logoClass" value="logo_ruready" />
		<put name="toolbar" value="/WEB-INF/common/tiles/toolbar.jsp" />
		<put name="content" value="/WEB-INF/common/tiles/content.jsp" />
		<put name="links" value="/WEB-INF/common/tiles/links.jsp" />
		<put name="footer" value="/WEB-INF/common/tiles/footer.jsp" />
	</definition>
	
	<!-- Transcript Views -->
	
	<definition name=".transcriptBaseDef" extends=".commonDef">
		<put name="toolbar" value="/WEB-INF/ta/tiles/viewTranscript.toolbar.jsp"/>
	</definition>
	
	<definition name=".transcriptPreAssessment" extends=".transcriptBaseDef">
		<put name="title" value="ta.viewTranscriptPreAssessment.title"/>
		<put name="messageKeyPrefix" value="ta.viewTranscriptPreAssessment"/>
		<put name="head" value="/WEB-INF/ta/jsp/viewTranscript.head.jsp"/>
		<put name="content" value="/WEB-INF/ta/jsp/viewTranscript.assessments.jsp"/>
	</definition>
	
	<definition name=".transcriptFull" extends=".transcriptBaseDef">
		<put name="title" value="ta.viewTranscript.title"/>
		<put name="messageKeyPrefix" value="ta.viewTranscript"/>
		<put name="head" value="/WEB-INF/ta/jsp/viewTranscript.head.jsp"/>
		<put name="content" value="/WEB-INF/ta/jsp/viewTranscript.assessments.jsp"/>
		<put name="secondContent" value="/WEB-INF/ta/jsp/viewTranscript.practicesAndFeedback.jsp"/>
	</definition>
	
	<definition name=".transcriptReadOnly" extends=".transcriptBaseDef">
		<put name="title" value="ta.viewTranscriptReadOnly.title"/>
		<put name="messageKeyPrefix" value="ta.viewTranscriptReadOnly"/>
		<put name="content" value="/WEB-INF/ta/jsp/viewTranscript.assessments.readOnly.jsp"/>
		<put name="secondContent" value="/WEB-INF/ta/jsp/viewTranscript.practicesAndFeedback.readOnly.jsp"/>
	</definition>
	
	<!-- Expectation Assessment Views -->
	<definition name=".expectationAssessmentBaseDef" extends=".commonDef">
		<put name="toolbar" value="/WEB-INF/ta/tiles/expectationAssessment.toolbar.jsp"/>
	</definition>
	
	<definition name=".expectationAssessment.entryForm" extends=".expectationAssessmentBaseDef">
		<put name="title" value="ta.expectationAssessment.entryForm.title"/>
		<put name="head" value="/WEB-INF/ta/jsp/expectationAssessment.entryForm.head.jsp"/>
		<put name="content" value="/WEB-INF/ta/jsp/expectationAssessment.entryForm.jsp"/>
	</definition>
	
	<definition name=".expectationAssessment.resultsForm" extends=".commonDef">
		<put name="title" value="ta.expectationAssessment.resultsForm.title"/>
		<put name="content" value="/WEB-INF/ta/jsp/expectationAssessment.resultsForm.jsp"/>
	</definition>
	
	<!-- Knowledge Assessment Views -->
	<definition name=".knowledgeAssessmentBaseDef" extends=".commonDef">
		<put name="toolbar" value="/WEB-INF/ta/tiles/assessment.toolbar.jsp"/>
	</definition>
	
	<definition name=".knowledgeAssessment.instructions" extends=".commonDef">
		<put name="title" value="ta.knowledgeAssessment.instructions.title"/>
		<put name="head" value="/WEB-INF/ta/jsp/assessment.instructions.head.jsp"/>
		<put name="content" value="/WEB-INF/ta/jsp/knowledgeAssessment.instructions.jsp"/>
	</definition>
	
	<definition name=".knowledgeAssessment.multipleChoice.entryForm" extends=".knowledgeAssessmentBaseDef">
		<put name="title" value="ta.knowledgeAssessment.entryForm.title"/>
		<put name="head" value="/WEB-INF/ta/jsp/knowledgeAssessment.multipleChoice.entryForm.head.jsp"/>
		<put name="content" value="/WEB-INF/ta/jsp/knowledgeAssessment.multipleChoice.entryForm.jsp"/>
	</definition>
	
	<definition name=".knowledgeAssessment.openEnded.entryForm" extends=".knowledgeAssessmentBaseDef">
		<put name="title" value="ta.knowledgeAssessment.entryForm.title"/>
		<put name="head" value="/WEB-INF/ta/jsp/knowledgeAssessment.openEnded.entryForm.head.jsp"/>
		<put name="content" value="/WEB-INF/ta/jsp/knowledgeAssessment.openEnded.entryForm.jsp"/>
	</definition>
	
	<definition name=".knowledgeAssessment.resultsForm" extends=".commonDef">
		<put name="title" value="ta.knowledgeAssessment.resultsForm.title"/>
		<put name="content" value="/WEB-INF/ta/jsp/assessment.resultsForm.jsp"/>
	</definition>
	
	<!-- Practice Assessment Views -->
	<definition name=".practiceAssessmentBaseDef" extends=".commonDef">
		<put name="toolbar" value="/WEB-INF/ta/tiles/assessment.toolbar.jsp"/>
	</definition>
	
	<definition name=".practiceAssessment.instructions" extends=".commonDef">
		<put name="title" value="ta.practiceAssessment.instructions.title"/>
		<put name="head" value="/WEB-INF/ta/jsp/assessment.instructions.head.jsp"/>
		<put name="content" value="/WEB-INF/ta/jsp/practiceAssessment.instructions.jsp"/>
	</definition>
	
	<definition name=".practiceAssessment.entryForm" extends=".practiceAssessmentBaseDef">
		<put name="title" value="ta.practiceAssessment.entryForm.title"/>
		<put name="head" value="/WEB-INF/ta/jsp/practiceAssessment.entryForm.head.jsp"/>
		<put name="content" value="/WEB-INF/ta/jsp/practiceAssessment.entryForm.jsp"/>
	</definition>
	
	<definition name=".practiceAssessment.feedback" extends=".practiceAssessmentBaseDef">
		<put name="title" value="ta.practiceAssessment.feedback.title"/>
		<put name="head" value="/WEB-INF/ta/jsp/practiceAssessment.feedback.head.jsp"/>
		<put name="content" value="/WEB-INF/ta/jsp/practiceAssessment.feedback.jsp"/>
	</definition>
	
	<definition name=".practiceAssessment.resultsForm" extends=".commonDef">
		<put name="title" value="ta.practiceAssessment.resultsForm.title"/>
		<put name="content" value="/WEB-INF/ta/jsp/assessment.resultsForm.jsp"/>
	</definition>
	
	<!-- View History Views -->
	<definition name=".viewHistory.assessmentList" extends=".commonDef">
		<put name="content" value="/WEB-INF/ta/jsp/viewHistory.assessment.list.jsp"/>
	</definition>
	
	<definition name=".viewHistory.assessmentList.expectationAssessment" extends=".viewHistory.assessmentList">
		<put name="title" value="ta.viewHistory.assessmentList.expectationAssessment.title"/>
		<put name="messageKeyPrefix" value="ta.viewHistory.assessmentList.expectationAssessment"/>
	</definition>
	
	<definition name=".viewHistory.assessmentList.knowledgeAssessment" extends=".viewHistory.assessmentList">
		<put name="title" value="ta.viewHistory.assessmentList.knowledgeAssessment.title"/>
		<put name="messageKeyPrefix" value="ta.viewHistory.assessmentList.knowledgeAssessment"/>
	</definition>
	
	<definition name=".viewHistory.assessmentDetails.expectationAssessment" extends=".commonDef">
		<put name="title" value="ta.viewHistory.expectationAssessment.results.title"/>
		<put name="messageKeyPrefix" value="ta.viewHistory.expectationAssessment.results"/>
		<put name="content" value="/WEB-INF/ta/jsp/viewHistory.expectationAssessment.details.jsp"/>
	</definition>
	
	<definition name=".viewHistory.assessmentDetails.knowledgeAssessmentBaseDef" extends=".commonDef">
		<put name="content" value="/WEB-INF/ta/jsp/viewHistory.knowledgeAssessment.details.jsp"/>
	</definition>
	
	<definition name=".viewHistory.assessmentDetails.knowledgeAssessment" extends=".viewHistory.assessmentDetails.knowledgeAssessmentBaseDef">
		<put name="title" value="ta.viewHistory.knowledgeAssessment.results.title"/>
		<put name="messageKeyPrefix" value="ta.viewHistory.knowledgeAssessment.results"/>
	</definition>
</tiles-definitions>
